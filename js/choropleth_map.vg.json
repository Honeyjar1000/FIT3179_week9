{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "title": "Average Salary 2014",
  "width": 800,
  "height": 600,
  "data": {
    "url": "https://raw.githubusercontent.com/Honeyjar1000/FIT3179/main/js/worldmap.geo.json",
    "format": {
          "type": "topojson",
          "feature": "worldmap"
        }
  },
  "transform": [{
      "lookup": "properties.NAME",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/Honeyjar1000/main/data/After_tax_plus_year_gross.csv"
        },
        "key": "Country",
        "fields": ["Year Gross Salary"]
      }
    }
  ],
  "mark": {
    "type": "geoshape"
    },
  "encoding": {
    "color": {
      "field": "Active Cases",
      "type": "quantitative"
    },
    "tooltip": [
      {"field": "properties.NAME", "type": "nominal", "title": "Country"},
      {"field": "Active", "type": "quantitative"}
    ]
  }

}
